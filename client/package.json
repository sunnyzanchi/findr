{
  "name": "findr-client",
  "version": "0.1.0",
  "description": "The client side for Findr",
  "main": "index.js",
  "author": "Zanchi <emzanchi@gmail.com> (https://github.com/zanchi)",
  "license": "MIT",
  "scripts": {
    "build": "parcel build ./src/index.html --no-source-maps",
    "build:dev": "parcel build ./src/index.html",
    "lint": "eslint src/",
    "start": "parcel --open -p 3000 ./src/index.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": "last 1 Firefox version",
  "browser": {
    "react": "preact"
  },
  "postcss": {
    "modules": true,
    "plugins": [
      "postcss-nested"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preact": "^6.5.15",
    "babel-loader": "^8.3.0",
    "parcel": "^2.8.2",
    "postcss-modules": "^6.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^13.2.0"
  },
  "dependencies": {
    "astroturf": "^1.1.0",
    "preact": "^10.4.0",
    "preact-router": "^3.2.1",
    "reset-css": "^5.0.1"
  }
}
